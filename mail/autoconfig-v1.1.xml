<?xml version="1.0" encoding="UTF-8"?>

<clientConfig version="1.1">
  <emailProvider id="magicnet.org">
    <domain>magicnet.org</domain>
    <displayName>Magicnet ORG</displayName>
    <displayShortName>Magicnet</displayShortName>
    <incomingServer type="imap">
      <hostname>infra.mn.magicnet.org</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <authentication>password-encrypted</authentication>
      <username>%EMAILADDRESS%</username>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>infra.mn.gm-hosting.net</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <authentication>password-encrypted</authentication>
      <username>%EMAILADDRESS%</username>
    </outgoingServer>
  </emailProvider>
</clientConfig>
